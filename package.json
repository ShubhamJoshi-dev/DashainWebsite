{
  "name": "dashain-festival-website",
  "version": "1.0.0",
  "description": "Dashain Festival - The Greatest Festival of Nepal - Next.js Application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next build && next export",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "preview": "npm run build && npm run start",
    "deploy:vercel": "vercel --prod",
    "deploy:static": "npm run export"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^10.16.0",
    "next": "^14.0.0",
    "next-seo": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "dashain",
    "nepal",
    "festival",
    "culture",
    "next.js",
    "react"
  ],
  "author": "Dashain Festival Website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/dashain-website"
  }
}
